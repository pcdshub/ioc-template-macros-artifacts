#
# unixCam IOC archive substitutions file
#

# Generate generic portion of unixCam archive request 
file archive_unixCam.archive
{
        {
                IOC             =       MFX:REC:IOC:01,
                EVR             =       MFX:REC:EVR:01
        }
}

# Generate one set per camera
file archive_edt_unix.archive
{
        {       CAM             =       MFX:DG1:CVV:01  }
        {       CAM             =       MFX:DG2:CVV:01  }
}

