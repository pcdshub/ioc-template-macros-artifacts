# Generate autosave request 
file ims_autosave.tpl-req
{
    pattern { MOTOR      }
            
            
            
            
            { CXI:DS1:MMS:07 }
            { CXI:DS1:MMS:08 }
            
            
            
            
            
            
            
            { CXI:DS1:MMS:10  }
            
            
            { CXI:DS1:MMS:09   }
            
            { CXI:DS1:MMS:11   }
            }

file device_with_2states.tpl-req
{
    pattern { DEVICE,        STATE1, STATE2 }
            
            
            
            
            { CXI:DS1:ATT:INOUT,        "IN",   "OUT"  }
            
            }

